{
  "name": "vereador-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run vereador-website:serve-ssr",
    "serve:ssr": "node dist/vereador-website/server/main.js",
    "build:ssr": "ng build --prod && ng run vereador-website:server:production",
    "prerender": "ng run vereador-website:prerender",
    "build:functions": "npm run --prefix functions build",
    "build:all": "npm run build:ssr && npm run build:functions",
    "deploy": "npm run build:all && firebase deploy --only functions:ssr && firebase deploy --only hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/fire": "^6.0.3",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.5",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/platform-server": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@nguniversal/express-engine": "^10.0.2",
    "express": "^4.15.2",
    "firebase": "^7.21.1",
    "lottie-web": "^5.7.3",
    "ngx-lottie": "^6.4.0",
    "ngx-mask": "^10.0.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
